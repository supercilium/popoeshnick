{
    "parser": "babel-eslint",
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended"
    ],
    "env": {
      "browser": true,
      "node": true,
      "es2017": true
    },
    "parserOptions": {
      "ecmaVersion": 6,
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "rules": {
        "linebreak-style": 0,
        "semi": [2, "never"],
        "import/no-named-as-default": 0,
        "import/prefer-default-export": 0,
        "react/destructuring-assignment": 0,
        "react/forbid-prop-types": [0, { "forbid": ["any"] }]
    },
    "reportUnusedDisableDirectives": true
}
